{
  "abi": 5,
  "ruleset": [
    {
      "handledAccessFs": [
        "abi.all"
      ],
      "handledAccessNet": [
        "bind_tcp"
      ]
    }
  ],
  "pathBeneath": [
    {
      "allowedAccess": [
        "abi.read_execute"
      ],
      "parent": [
        ".",
        "/bin",
        "/lib",
        "/usr",
        "/dev",
        "/etc",
        "/proc"
      ]
    },
    {
      "allowedAccess": [
        "abi.read_write"
      ],
      "parent": [
        "/tmp"
      ]
    }
  ],
  "netPort": [
    {
      "allowedAccess": [
        "bind_tcp"
      ],
      "port": [
        80,
        443
      ]
    }
  ]
}
